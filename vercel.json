{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "whisper_worker.js", "use": "@vercel/static" },
    { "src": "whisper/**", "use": "@vercel/static" },
    { "src": "models/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
